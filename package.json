  "repository": {
    "type": "git",
    "url": "git@github.com:SomeKittens/gustav.git"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-alpha.10",
    "node-uuid": "1.4.7",
    "tail": "~0.4.0"
  },
  "devDependencies": {
    "chai": "~3.4.1",
    "mocha": "~2.3.4",
    "snyk": "~1.3.0",
    "tsd": "~0.6.5",
    "tslint": "~3.0.0",
    "typescript": "~1.7.3"
  },
  "engines": {
    "node": ">=4.x.x"
  }
