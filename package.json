    "test:watch": "mocha --watch dist/test",
    "lint": "tslint *.ts && tslint test/*.ts"
  },
  "author": "Randall Koutnik",
  "typescript": {
    "definition": "index.d.ts"
  },
  "bugs": {
    "url": "https://github.com/SomeKittens/gustav/issues"
  },
